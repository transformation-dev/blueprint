{
    "eslint.validate": [
        "javascript",
        "html",
        "svelte"
    ],
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "files.associations": {
        "*.svelte": "svelte"
    },
    "typescript.validate.enable": false,
    "javascript.validate.enable": true,
    "search.exclude": {
        "**/node_modules": true,
        "dist/": true
    },
    "files.exclude": {
        "**/node_modules": true
    },
    "[html]": {
        "editor.tabSize": 2
    },
    "[svelte]": {
        "editor.tabSize": 2
    },
    "[scss]": {
        "editor.tabSize": 2
    },
    "[yaml]": {
        "editor.tabSize": 2
    },
    "[javascript]": {
        "editor.tabSize": 2
    },
    "[json]": {
        "editor.tabSize": 2
    },
    "debug.internalConsoleOptions": "neverOpen"
}
