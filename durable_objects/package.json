{
  "name": "@transformation-dev/durable_objects",
  "version": "1.0.0",
  "description": "Durable objects for Transformation.dev blueprint",
  "main": "./index.mjs",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "publish": "npm run build && npx wrangler publish",
    "build": "npx esbuild src/index.js --sourcemap --outfile=index.mjs --format=esm --target=esnext --bundle --loader:.yaml=text && npm run test",
    "dev": "npx esbuild src/index.js --sourcemap --outfile=index.mjs --format=esm  --bundle --loader:.yaml=text --watch",
    "test": "tape test/* | tap-spec",
    "coverage": "c8 npm run test",
    "coverage-report": "c8 report --reporter=html"
  },
  "dependencies": {
    "@cfworker/json-schema": "^1.12.5",
    "@transformation-dev/accept": "file:packages/accept",
    "cbor-x": "^1.4.1",
    "debug": "^4.3.3",
    "deep-object-diff": "^1.1.9",
    "nanoid": "^4.0.0",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "esbuild": "^0.17.2",
    "supports-color": "^9.3.1",
    "tap-spec": "^5.0.0",
    "tape": "^5.6.1",
    "wrangler": "^2.6.2"
  }
}
